{
  "name": "matty-ice-app-template",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "scripts": {
    "frontend:dev": "pnpm --filter @matty-ice-app-template/react-frontend dev",
    "frontend:build": "npm run build:client && npm run build:server",
    "frontend:build:client": "vite build --ssrManifest --outDir dist/client",
    "frontend:build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "frontend:preview": "cross-env NODE_ENV=production node server",
    "frontend:lint": "eslint --max-warnings=0 src",
    "frontend:typecheck": "tsc"
  },
  "devDependencies": {},
  "dependencies": {},
  "packageManager": "pnpm@8.11.0"
}
